<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  <Id>00000000-0000-0000-0000-000000000000</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Erik Plachta</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Excel Extension Sample"/>
  <Description DefaultValue="Sample manifest pointing to GitHub Pages deployment"/>
  <SupportUrl DefaultValue="https://github.com/Erikplachta/excel-extension/issues"/>
  <IconUrl DefaultValue="https://Erikplachta.github.io/excel-extension/favicon.svg"/>
  <HighResolutionIconUrl DefaultValue="https://Erikplachta.github.io/excel-extension/icon-512.svg"/>
  <AppDomains>
    <AppDomain>https://Erikplachta.github.io</AppDomain>
    <AppDomain>https://Erikplachta.github.io/excel-extension</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://Erikplachta.github.io/excel-extension/index.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>
