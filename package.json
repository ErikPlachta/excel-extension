{
  "name": "excel-extension",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "npm run start:dev && npm run start:office",
    "start:office": "office-addin-debugging start dev-manifest.xml",
    "stop:office": "office-addin-debugging stop dev-manifest.xml",
    "dev-certs": "node scripts/install-dev-certs.mjs",
    "start:dev": "ng serve --configuration development --ssl true --ssl-cert /Users/erikplachta/.office-addin-dev-certs/localhost.crt --ssl-key /Users/erikplachta/.office-addin-dev-certs/localhost.key",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts,.html",
    "lint:office": "office-addin-lint check",
    "lint:office:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "validate:dev-manifest": "npx --yes office-addin-manifest validate dev-manifest.xml"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.12",
    "@angular/compiler": "^20.3.12",
    "@angular/core": "^20.3.12",
    "@angular/forms": "^20.3.12",
    "@angular/platform-browser": "^20.3.12",
    "@angular/platform-browser-dynamic": "^20.3.12",
    "@angular/router": "^20.3.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.10",
    "@angular-eslint/eslint-plugin": "^20.3.12",
    "@angular-eslint/eslint-plugin-template": "^20.3.12",
    "@angular-eslint/template-parser": "^20.3.12",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.12",
    "@types/jasmine": "~5.1.0",
    "@types/office-js": "^1.0.305",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-plugin-office-addins": "^4.0.3",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=20 <25"
  }
}
