{
  "name": "excel-extension",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "npm run start:dev && npm run start:office",
    "start:office": "office-addin-debugging start dev-manifest.xml",
    "stop:office": "office-addin-debugging stop dev-manifest.xml",
    "dev-certs": "node scripts/install-dev-certs.mjs",
    "start:dev": "ng serve --configuration development --ssl true --ssl-cert /Users/erikplachta/.office-addin-dev-certs/localhost.crt --ssl-key /Users/erikplachta/.office-addin-dev-certs/localhost.key",
    "start:mock": "ng serve --configuration mock",
    "test:api": "./scripts/test-local-api.sh",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "nx test excel-addin",
    "test:ci": "nx run-many --target=test --all --ci",
    "lint": "eslint . --ext .ts,.html",
    "lint:office": "office-addin-lint check",
    "lint:office:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "validate:dev-manifest": "npx --yes office-addin-manifest validate dev-manifest.xml",
    "nx": "nx",
    "graph": "nx graph",
    "affected": "nx affected",
    "affected:lint": "nx affected --target=lint",
    "affected:test": "nx affected --target=test",
    "affected:build": "nx affected --target=build",
    "ci": "nx affected --target=lint --parallel=3 && nx affected --target=test --parallel=3 --ci && nx affected --target=build --parallel=3",
    "ci:all": "nx run-many --target=lint --all --parallel=3 && nx run-many --target=test --all --ci --parallel=3 && nx run-many --target=build --all --parallel=3",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "test:watch": "nx test excel-addin --watch",
    "docs": "cd apps/excel-addin-docs-website && npm run build",
    "docs:serve": "cd apps/excel-addin-docs-website && npm start",
    "docs:generate": "node scripts/generate-docs.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "office-addin-manifest": "^2.1.2",
    "office-addin-sso": "^3.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zod": "^4.1.13",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular-eslint/eslint-plugin-template": "^21.0.1",
    "@angular-eslint/template-parser": "^21.0.1",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@compodoc/compodoc": "^1.1.32",
    "@nx/angular": "^22.1.3",
    "@nx/eslint": "^22.1.3",
    "@nx/jest": "^22.1.3",
    "@nx/workspace": "^22.1.3",
    "@types/jest": "^30.0.0",
    "@types/office-js": "^1.0.561",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-plugin-office-addins": "^4.0.3",
    "eslint-plugin-tsdoc": "^0.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "nx": "^22.1.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=20 <=24"
  }
}
